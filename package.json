{
  "name": "vue3_vite_ts_basic",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "cm": "git-cz",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "deploy": "gh-pages -d dist",
    "format": "prettier --write ./src",
    "lint": "eslint ./src --ext .vue,.js,.ts,.tsx",
    "lint:fix": "eslint --fix ./src --ext .vue,.js,.ts,.tsx",
    "lint:eslint": "eslint \"{src,mock}/**/*.{vue,ts,tsx}\" --fix",
    "lint:prettier": "prettier --write --loglevel warn \"src/**/*.{js,json,tsx,css,less,scss,vue,html,md}\"",
    "lint:stylelint": "stylelint --fix **/*.{vue,less,postcss,css,scss} --cache --cache-location node_modules/.cache/stylelint/",
    "lint:lint-staged": "lint-staged -c ./.husky/lintstagedrc.js",
    "lint:pretty": "pretty-quick --staged",
    "prepare": "husky install"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@onlyoffice/document-editor-vue": "^1.3.0",
    "@turf/turf": "^6.5.0",
    "@wangeditor/editor": "^5.1.23",
    "@wangeditor/editor-for-vue": "^5.1.12",
    "ant-design-vue": "^3.2.20",
    "axios": "^1.5.0",
    "cesium": "^1.107.2",
    "cesium-tdt": "^1.1.0",
    "clipboard": "^2.0.11",
    "csv-parse": "^5.5.0",
    "echarts": "^5.4.3",
    "echarts-stat": "^1.2.0",
    "file-saver": "^2.0.5",
    "gdal3.js": "^2.6.0",
    "geojson": "^0.5.0",
    "jszip": "^3.10.1",
    "lil-gui": "^0.18.2",
    "lodash": "^4.17.21",
    "maplibre-gl": "^3.6.2",
    "moment": "^2.29.4",
    "nanoid": "^4.0.2",
    "ol": "^7.4.0",
    "ol-ext": "^4.0.11",
    "ol-mapbox-style": "^10.6.0",
    "pinia": "^2.1.6",
    "satellite.js": "^5.0.0",
    "snabbdom": "^3.5.1",
    "vue": "^3.2.47",
    "vue-router": "^4.2.4",
    "vue3-colorpicker": "^2.2.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@types/cesium": "^1.70.0",
    "@types/ol": "^7.0.0",
    "@types/ol-ext": "npm:@siedlerchr/types-ol-ext",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^7.0.0",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.15.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "postcss": "^8.4.24",
    "postcss-html": "^1.5.0",
    "postcss-less": "^6.0.0",
    "postcss-scss": "^4.0.6",
    "prettier": "^2.8.8",
    "pretty-quick": "^3.1.3",
    "sass": "^1.63.4",
    "stylelint": "^15.8.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-recommended-scss": "^12.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^33.0.0",
    "stylelint-order": "^6.0.3",
    "stylelint-scss": "^5.0.1",
    "typescript": "^5.0.2",
    "unplugin-vue-components": "^0.25.1",
    "vite": "^4.3.9",
    "vite-plugin-cesium": "^1.2.22",
    "vue-eslint-parser": "^9.3.1",
    "vue-tsc": "^1.4.2"
  },
  "lint-staged": {
    "*.{vue,js,ts,tsx}": "eslint --fix"
  },
  "peerDependencies": {}
}
